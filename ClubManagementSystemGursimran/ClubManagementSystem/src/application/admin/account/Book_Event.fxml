<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane prefHeight="700.0" prefWidth="605.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.admin.account.Book_Event_Controller">
	<content>
		<AnchorPane prefHeight="700.0" prefWidth="600.0">
			<children>
				<Text layoutX="30.0" layoutY="94.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Event Name:">
					<font>
						<Font size="14.0" />
					</font>
				</Text>
				<Text layoutX="30.0" layoutY="140.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Organisers:">
					<font>
						<Font size="14.0" />
					</font>
				</Text>
				<Text layoutX="34.0" layoutY="260.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Description" />
				<Text layoutX="31.0" layoutY="439.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Team size:">
					<font>
						<Font size="14.0" />
					</font>
				</Text>
				<TextField fx:id="BookEventName" layoutX="200.0" layoutY="73.0"
					prefHeight="35.0" prefWidth="300.0" promptText="What is the name of your event? ">
					<font>
						<Font size="14.0" />
					</font>
				</TextField>
				<TextArea fx:id="BookEventOrganisers" layoutX="200.0"
					layoutY="128.0" prefHeight="94.0" prefWidth="299.0"
					promptText="Who are the organisers of your event?">
					<font>
						<Font size="14.0" />
					</font>
				</TextArea>
				<TextArea fx:id="BookEventDescription" layoutX="200.0"
					layoutY="244.0" prefHeight="100.0" prefWidth="302.0" promptText="Decribe your Event" />
				<Label layoutX="30.0" layoutY="10.0" prefHeight="46.0"
					prefWidth="511.0" text="IIIT-Delhi Club Event Booking Form"
					textFill="#3fada8" underline="true">
					<font>
						<Font name="Arial Bold" size="30.0" />
					</font>
				</Label>
				<ComboBox fx:id="BookEventTeamSize" layoutX="201.0"
					layoutY="417.0" prefHeight="35.0" prefWidth="300.0"
					promptText="Team size allowed for the event?" />
				<Text layoutX="31.0" layoutY="544.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Prize:">
					<font>
						<Font size="14.0" />
					</font>
				</Text>
				<TextArea fx:id="BookEventPrize" layoutX="201.0" layoutY="530.0"
					prefHeight="94.0" prefWidth="299.0"
					promptText="Mention prize details of your event? What is the prize for the winner? What about runner-ups? Is there any consolation prize?">
					<font>
						<Font size="14.0" />
					</font>
				</TextArea>
				<Button fx:id="bookEvent" layoutX="313.0" layoutY="647.0"
					mnemonicParsing="false" onAction="#bookEventAction" prefHeight="35.0"
					prefWidth="90.0" text="Book Event">
					<font>
						<Font size="14.0" />
					</font>
				</Button>
				<Button fx:id="cancelEventBooking" layoutX="201.0" layoutY="647.0"
					mnemonicParsing="false" onAction="#cancelEventBookingAction"
					prefHeight="35.0" prefWidth="90.0" text="Cancel">
					<font>
						<Font size="14.0" />
					</font>
				</Button>
				<Text layoutX="30.0" layoutY="494.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Budget:">
					<font>
						<Font size="14.0" />
					</font>
				</Text>
				<TextField fx:id="BookEventBudget" layoutX="201.0"
					layoutY="474.0" prefHeight="35.0" prefWidth="300.0"
					promptText="What is the budget of your event?">
					<font>
						<Font size="14.0" />
					</font>
				</TextField>
				<Text fx:id="BookEventError" fill="RED" layoutX="264.0"
					layoutY="723.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Invalid Input"
					textAlignment="CENTER" visible="false" wrappingWidth="172.02197265625">
					<font>
						<Font name="System Bold" size="15.0" />
					</font>
				</Text>
				<Text fx:id="errorDetails" layoutX="201.0" layoutY="760.0"
					strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="326.6708984375" />
				<Text layoutX="34.0" layoutY="385.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Organising Club" />
				<ComboBox fx:id="BookEventOrganisingClub" layoutX="200.0"
					layoutY="363.0" prefHeight="31.0" prefWidth="298.0"
					promptText="Which Club is Organising This Event?" />
			</children>
		</AnchorPane>
	</content>
</ScrollPane>
