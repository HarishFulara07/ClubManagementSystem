<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane prefHeight="505.0" prefWidth="555.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.admin.login.Admin_Login_Screen_Controller">
	<content>
		<AnchorPane prefHeight="500.0" prefWidth="550.0">
			<children>
				<TextField fx:id="adminEmail" layoutX="131.0" layoutY="156.0"
					prefHeight="35.0" prefWidth="288.0" promptText="Enter your email" />
				<Label layoutX="21.0" layoutY="98.0" text="Admin LogIn"
					underline="true">
					<font>
						<Font name="Arial Bold" size="24.0" />
					</font>
				</Label>
				<Button fx:id="adminLoginSubmit" layoutX="131.0" layoutY="274.0"
					mnemonicParsing="false" onAction="#adminLoginSubmitAction" text="Submit" />
				<Button fx:id="adminLoginBack" layoutX="21.0" layoutY="413.0"
					mnemonicParsing="false" onAction="#homeScreenFromAdmin" text="Back To Home Page" />
				<PasswordField fx:id="adminPassword" layoutX="131.0"
					layoutY="215.0" prefHeight="35.0" prefWidth="288.0" promptText="Password" />
				<Label layoutX="21.0" layoutY="20.0" prefHeight="46.0"
					prefWidth="524.0" text="IIIT-Delhi Club Management System"
					textFill="#3fada8" underline="true">
					<font>
						<Font name="Arial Bold" size="30.0" />
					</font>
				</Label>
				<Text fx:id="adminLoginError" fill="#c90d0d" layoutX="153.0"
					layoutY="350.0" strokeType="OUTSIDE" strokeWidth="0.0"
					text="Invalid Email or Password" textAlignment="CENTER" visible="false"
					wrappingWidth="243.3046875">
					<font>
						<Font name="Arial Bold" size="16.0" />
					</font>
				</Text>
				<Text fx:id="errorDetails" layoutX="238.0" layoutY="388.0"
					strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="270.3046875" />
			</children>
		</AnchorPane>
	</content>
</ScrollPane>
